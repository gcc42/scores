{
  "name": "scores",
  "description": "A tool to organize (TV and movie) soundtracks into Spotify playlists.",
  "version": "0.0.1",
  "author": "gcc42",
  "bin": {
    "scores": "./bin/run"
  },
  "bugs": "https://github.com/gcc42/scores/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.15",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.51",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "chai": "^4.3.0",
    "clang-format": "^1.5.0",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "got": "^11.8.1",
    "jsdom": "^16.4.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9",
    "typescript-eslint": "^0.0.1-alpha.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/gcc42/scores",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "scores"
  },
  "repository": "gcc42/scores",
  "scripts": {
    "es-fix": "eslint \"{src,test}/**/*.{js,ts}\" --fix",
    "format": "clang-format --style=file -i --glob=\"{src,test}/**/*.{js,ts}\"",
    "fix": "run-s format es-fix",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rimraf lib && tsc -b && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "tslib": "^1.14.1"
  }
}
